[user]
	name = Jan Joseph Concepcion
	email = jconcep2@mail.sfsu.edu
[core]
	editor = vim
	autocrlf = input
	safecrlf = true
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  type = cat-file -t
  dump = cat-file -p
